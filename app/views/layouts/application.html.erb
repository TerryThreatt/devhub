<!DOCTYPE html>
<html>
  <head>
    <title>Devhub</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

    <%= render partial: "shared/nav" %>
    <%= render partial: "shared/alerts" %>

    <%= yield :header %>
    <%= yield :content %>
    <%= yield %>
    <%= yield :footer %>
  </body>
</html>
