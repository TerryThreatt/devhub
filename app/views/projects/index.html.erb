<h1><%= @user.email %> 's Projects</h1>

<% content_for :content %>
    <% if @projects.any? %>
        <% @projects.each do |project| %>
            <p>Name:<%= link_to project.name, project_path(project) %><p>
            <p>Description:<%= link_to project.description, project_path(project) %><p>
        <% end %>
    <% end %>

<%= link_to 'Create New Project', new_project_path %>
